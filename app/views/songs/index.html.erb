<section id="audio"></section>

<ul>  
<% @songs.each do |song| %>  
   <li><%= song.key %> - <td><%= link_to "Download", download_url_for(song.key) %></td>
   <%= link_to "Delete",  "delete/?song=" + song.key, :confirm => 'Are you sure you want to delete ' + song.key + '?' %></li>  
<td><%= link_to "HTML5 Audio", download_url_for(song.key), :class => "html5" %></td>
   <% end %>  

</ul>  

<h2>Upload a new MP3:</h2>  
<%= form_tag upload_path, :method => "post", :multipart => true do %>  
    <%= file_field_tag "mp3file" %>  
    <%= submit_tag "Upload" %>  
<% end %> 